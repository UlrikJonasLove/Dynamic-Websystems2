@model IEnumerable<Tags>

@{
    var user = User.Identity.Name;
}

@if (Model != null)
{
    <div class="row tags-wrapper">
        @foreach (var c in Model)
        {
            <div class="col-xs-12 tags-block">
                <p class="w-100">#@c.Text</p>
            </div>
        }
    </div>
}
@if (Model.Count() == 0)
{
    <div class="alert alert-light">Tomt</div>
}